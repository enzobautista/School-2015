<section id="belowNav" class="u_log">
  <div class="container marg-cont">
  <div class="col-md-4"></div>
    <div class="card col-md-4 log_form">
      <h2>Log in as Admin</h2><hr>

      <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form-inputs">
          <%= f.input :email, required: false, autofocus: true %><br>
          <%= f.input :password, required: false %><br>
          <div class="rem">
          <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %></div>
        </div>

        <div class="form-actions">
        
          <%= button_to(type:'submit',class:"btn-default")do%>  
            <span class="glyphicon glyphicon-send"></span> Log in
          <%end%>
        </div>
      <% end %>
      <hr>

    </div>
  </div>
  